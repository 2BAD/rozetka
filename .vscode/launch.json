  "version": "0.2.0",
  "configurations": [
{
      "name": "Launch",
      "request": "launch",
      "type": "pwa-node",
      "program": "${file}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "--experimental-specifier-resolution=node",
        "--loader=ts-node/esm",
        "--require=dotenv/config",
        "--inspect-brk"
      ]
    }
  ]
}